{
    "layer": "top",
    // "position": "top",
    // "height": 30,
    // "spacing": 0,
    "reload_style_on_change": true,
    "modules-center": [
        "hyprland/workspaces",
        // "wlr/taskbar",
        "hyprland/window"
    ],
    "modules-left": [
        // "image#nixos",
        "custom/wallpaper",
        "cpu",
        "memory",
        "custom/gpu",
        "temperature",
        "network"
    ],
    "modules-right": [
        "tray",
        "idle_inhibitor",
        "bluetooth",
        "power-profiles-daemon",
        "pulseaudio",
        "backlight",
        "custom/notifications",
        "battery",
        "clock",
        "custom/power"
    ],
    "cpu": {
        "format": "ï‹› {usage}%"
    },
    "custom/gpu": {
        "interval": 5,
        "exec": "nvidia-smi --query-gpu=utilization.gpu --format=csv,noheader,nounits",
        "format": "ó°¢® {}%",
        "tooltip": false
    },
    "memory": {
        "format": "ó°¾… {used}/{total} GB", //ïƒ‰
        "tooltip": false
    },
    "temperature": {
        "format": "ï‹‰ {temperatureC}Â°C",
        //"critical-threshold": 80,
        //"format-critical": "{temperatureC}Â°C {icon}",
        "format-icons": [
            "ï‹‹",
            "ï‹Š",
            "ï‹‰",
            "ï‹ˆ",
            "ï‹‡"
        ],
        "tooltip": false
    },
    "backlight": {
        "format": "{percent}% {icon}",
        "min":3,
        "scroll-step": 3,
        "format-icons": [
            "î",
            "î“",
            "î‘",
            "î",
            "î",
            "î",
            "îŠ",
            "îˆ",
            "î›"
        ]
    },
    "network": {
        "format": "ó°¹¹ {bandwidthTotalBytes}",
        "format-disconnected": "Disconnected ó°–ª", //âš 
        "format-linked": "{ifname} (No IP) â€¼ï¸",
        "format-alt": "ï¢ {bandwidthUpBytes} | ï£ {bandwidthDownBytes}",
        "format-wifi": "{essid} ({signalStrength}%) ó°–©", //ï‡«
        "format-ethernet": "{ipaddr}/{cidr} ïƒ ",
        "tooltip-format": "{ifname} via {gwaddr} ï–",
    },
    "pulseaudio": {
        "format": "{volume}% {icon} ",
        "format-bluetooth": "{volume} ïŠ” {icon} ", //{format_source}
        "format-bluetooth-muted": "ó°Ÿ {icon} ïŠ”",
        "format-muted": "{volume}% ó°Ÿ",
        "format-source": "{volume}% ï„°",
        "format-source-muted": "ï„±",
        "tooltip-format": "{desc}",
        "scroll-step": 3,
        "on-click": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
        "format-icons": {
            "headphone": "ï€¥",
            "hands-free": "ï€¥",
            "headset": "ï€¥",
            "phone": "ï‚•",
            "portable": "ï‚•",
            "car": "ï†¹",
            "default": [
                "ï€¦",
                "ï€§",
                "ï€¨"
            ]
        },
        "on-click-right": "pavucontrol"
    },
    "tray": {
        "icon-size": 16,
        "spacing": 8
    },
    "clock": {
        "timezone": "Asia/Kolkata",
        "tooltip-format": "<big>{:%d/%m %A}</big>\n<tt><small>{calendar}</small></tt>",
        "format": "{:%H:%M}", //"{:%H:%M ó°ƒ­ %d %b %Y}"
        "format-alt": "{:%d-%m-%Y}"
    },
    "battery": {
        "states": {
            "good": 90,
            "warning": 30,
            "critical": 15
        },
        "format": "{capacity}% {icon}",
        "format-charging": "{capacity}% ï—§",
        "format-plugged": "{capacity}% ï‡¦",
        "format-alt": "{time} {icon}",
        // "format-good": "", // An empty format will hide the module
        // "format-full": "",
        "format-icons": [
            "ï‰„",
            "ï‰ƒ",
            "ï‰‚",
            "ï‰",
            "ï‰€"
        ]
    },
    "custom/notifications": {
        "tooltip": false,
        "format": "{icon}",
        "format-icons": {
            "notification": "ï‚¢ <span foreground='red'><sup>ï‘„</sup></span>", //ğŸ”” 
            "none": "ï‚¢",
            "dnd-notification": "ğŸ”•<span foreground='red'><sup>ï‘„</sup></span>",
            "dnd-none": "ğŸ”•"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "sleep 0.1; swaync-client -t -sw",
        "on-click-right": "sleep 0.1; swaync-client -d -sw",
        "escape": true
    },
    "hyprland/workspaces": {
        "disable-scroll": true,
        "format": "{name}", // {icon}
        "on-click": "activate",
        "format-icons": { // Could be good to develop that with hyprsome
            "urgent": "ó°—–",
            "active": "ó°¥",
            "default": "ó°¦"
        },
        "sort-by-number": true
    },
    "hyprland/window": {
        "format": "{}",
        "separate-outputs": true,
        "max-length": 32,
        "rewrite": {
            "(.*)kitty": "> [$1]",
            "(.*)Mozilla Firefox": "Firefox ó°ˆ¹",
            "(.*)Visual Studio Code": "Code ó°¨",
        }
    },
    "custom/power": {
        "format": "â»",
        "on-click": "rofi_powermenu"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "î©°",
            "deactivated": "î«§"
        },
        "tooltip": "true"
    },
    "bluetooth": {
        "format": "ïŠ“",
        "format-disabled": "ó°‚³",
        "format-connected": "ó°‚± {num_connections}",
        "tooltip-format": "ïŠ“ {device_alias}",
        "tooltip-format-connected": "{device_enumerate}",
        "tooltip-format-enumerate-connected": "ïŠ“ {device_alias} ó°‚„{device_battery_percentage}%",
        "tooltip-format-enumerate-disabled": "",
        "tooltip": true,
        "on-click": "blueman-manager",
    },
    "custom/wallpaper": {
        "format": "ïŒ“",
        "on-click": "wallpaper_random",
        // "on-click-right": "~/.config/hypr/scripts/WaybarStyles.sh",
        // "on-click-middle": "~/.config/hypr/scripts/randomwp.sh",
        "tooltip": false
    },
    "power-profiles-daemon": {
        "format": "{icon}",
        "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
        "tooltip": true,
        "format-icons": {
            "default": "ïƒ§",
            "performance": "ïƒ§",
            "balanced": "ï‰",
            "power-saver": "ï¬"
        }
    },
    //not used
    "image#nixos": {
        "path": "~/.config/waybar/nixos.png",
        "on-click": "wallpaper_random",
        "size": 24
    },
    "wlr/taskbar": {
        "format": "{icon}",
        "icon-size": 16,
        "all-outputs": false,
        "tooltip-format": "{title}",
        "on-click": "activate",
        "on-click-middle": "close",
        "ignore-list": [
            "rofi"
        ]
    },
    "keyboard-state": {
        "numlock": true,
        "capslock": true,
        "format": "{name} {icon}",
        "format-icons": {
            "locked": "ï€£",
            "unlocked": "ï‚œ"
        }
    },
    "pulseaudio/slider": {
        "min": 0,
        "max": 100,
        "orientation": "horizontal"
    },
    "disk": {
        "format": "ï‚  {used}/{total}",
        "format-alt": "ï‚  {percentage_used}% ({used})",
        "tooltip": false
    }
}
